{
	"name": "firebase-with-nextjs",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "NEXT_PUBLIC_USE_EMULATOR=true next dev --turbopack --experimental-https",
		"stage": "NEXT_PUBLIC_USE_EMULATOR=false next dev --turbopack --experimental-https",
		"build": "NODE_OPTIONS=--enable-source-maps next build",
		"start": "next start",
		"emulator": "firebase emulators:start --import=./emulator-data --export-on-exit=./emulator-data",
		"otel": "podman run --rm --name jaeger -p 16686:16686 -p 4318:4318 jaegertracing/jaeger:latest"
	},
	"dependencies": {
		"@google-cloud/pubsub": "^5.2.0",
		"@heroicons/react": "^2.2.0",
		"@heroui/react": "^2.8.6",
		"@heroui/theme": "^2.4.24",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
		"@react-aria/i18n": "^3.12.12",
		"@vercel/otel": "^2.1.0",
		"concurrently": "^9.2.1",
		"cookies-next": "^5.1.0",
		"firebase": "^12.4.0",
		"firebase-admin": "^13.2.0",
		"framer-motion": "^12.23.26",
		"js-cookie": "^3.0.5",
		"jwt-decode": "^4.0.0",
		"marked": "^16.3.0",
		"next": "^15.3.0",
		"postcss": "^8.5.6",
		"react": "^19.2.3",
		"react-dom": "^19.2.3"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.0",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.8.3"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
